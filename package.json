    "test:rabbitmq": "node scripts/test-rabbitmq-events.js"
  },
  "dependencies": {
    "@membership/shared-constants": "github:kashif147/membership-shared-constants#v1.0.0",
    "@projectShell/rabbitmq-middleware": "github:kashif147/rabbitmq-middleware#ec9abd6d0d4d64b0ddc6745c37b4f36acdbf2908",
    "amqplib": "^0.10.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv-flow": "^3.3.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "stripe": "^16.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^13.1.1",
    "supertest": "^7.1.4"
