{
  "name": "account-service",
  "version": "1.0.0",
  "type": "module",
  "main": "bin/account-service.js",
  "scripts": {
    "dev": "nodemon --watch src --watch bin --ext js,json --exec node bin/account-service.js",
    "start": "node bin/account-service.js",
    "start:dev": "cross-env NODE_ENV=development node bin/account-service.js",
